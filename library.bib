@article{Basu2003,
	Abstract = {This paper attempts to argue for the theory-ladenness of evidence. It does so by employing and analysing an episode from the history of eighteenth century chemistry. It delineates attempts by Joseph Priestley and Antoine Lavoisier to construct entirely different kinds of evidence for and against a particular hypothesis from a set of agreed upon observations or (raw) data. Based on an augmented version of a distinction, drawn by J. Bogen and J. Woodward, between data and phenomena it is shown that the role of theoretical auxiliary assumptions is very important in constructing evidence for (or against) a theory from observation or (raw) data. In revolutionary situations, rival groups hold radically different theories and theoretical auxiliary assumptions. These are employed to construct very different evidence from the agreed upon set of observations or (raw) data. Hence, theory resolution becomes difficult. It is argued that evidence construction is a multi-layered exercise and can be disputed at any level. What counts as unproblematic observation or (raw) data at one level may become problematic at another level. The contingency of these constructions and the (un)problematic nature of evidence are shown to be partially dependent upon the scientific knowledge that the scientific community possesses. ?? 2003 Elsevier Science Ltd. All rights reserved.},
	Author = {Basu, Prajit K.},
	Date-Added = {2018-02-02 00:27:21 +0000},
	Date-Modified = {2018-02-02 04:57:05 +0000},
	File = {:Users/nathanael/Documents/article database/Basu Theory-Ladenness of Evidence A Case Study from History.pdf:pdf},
	Issn = {00393681},
	Journal = {Studies in History and Philosophy of Science Part A},
	Keywords = {Construction of evidence,Eighteenth century chemical philosophy,Observation/data,Theory resolution,Theory-ladenness of evidence},
	Pages = {351--368},
	Title = {{Theory-ladenness of evidence: A case study from history of chemistry}},
	Volume = {34},
	Year = {2003},
	Bdsk-Url-1 = {https://dx.doi.org/10.1016/S0039-3681(03)00022-0}}

