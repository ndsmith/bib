@article{Fantl2002,
	Abstract = {Evidentialism holds that epistemic justification supervenes on evidence. But there are cases in which two subjects have the same evidence for a proposition but, because of pragmatic factors, only one is justified. Our argument against evidentialism is not based on intuitions about these cases. Rather, we aim to provide a theoretical basis for rejecting evidentialism by defending a pragmatic necessary condition on epistemic justification: S is justified in believing that p only if S is rational to prefer as if p. Therefore, the price of evidentialism is skepticism. Contextualism, as a form of evidentialism, is in the same boat.},
	Author = {Fantl, J. and McGrath, M.},
