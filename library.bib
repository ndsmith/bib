@incollection{schmitt_testimonial_2006,
	Abstract = {This paper argues that testimonial justification is social in a nonobvious sense: a hearer's belief on testimony can be justified on the basis of the reason the speaker possesses for believing the same proposition, even if the hearer does not possess that reason. I argue for this claim by analogy with an intuitively more plausible principle for memorial justification: a subject's belief can be justified on the basis of the reason the subject once possessed for the belief, even if the subject no longer possesses that reason. Tyler Burge's well known account of testimonial entitlement is inconsistent with what I say, so I criticize his account in defending my argument. I also criticize an attempt by David Owens to reconcile the position I defend with a social responsibilism about justification.},
	Author = {Frederick F Schmitt},
	Crossref = {Lackey2006-LACTEO},
	Date-Modified = {2018-02-01 03:54:15 +0000},
	Keywords = {justification, social, testimony},
