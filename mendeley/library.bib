@article{Canfield2001,
abstract = {The diary passage at Section 258 of Wittgenstein's <I>Philosophical Investigations<D> is often considered the key to his "private language argument" (or PLA). This paper attempts to provide a clear and persuasive account of Section 258. After some observations about method, the gist of which is that the PLA is a myth, and about the context in which Section 258 occurs, I consider briefly its initial statements, <I>a<D> through <I>l<D>. My concluding sentence by sentence and between the sentences account of the famous last four lines of Section 258 include a reply to the major substantive objection to that part of the <I>Investigations<D>. The following are among the exegetical questions addressed. Wittgenstein says that the would-be private diarist has no criterion of correctness; but what justifies the presumption that one is needed? Furthermore, why does Wittgenstein think none is available? This crucial question gets different answers in the secondary literature; I argue that the standard ones fail. The interpretation of Section 258 offered here is a version of the meaning-based readings found in Kenny and Candlish, among others. Its key supposition is that statements about the past must be governed by criteria concerning the present or future.},
author = {Canfield, J. V.},
doi = {10.1080/713659266},
file = {:Users/nathanael/Documents/article database/Canfield Private Language The Diary Case.pdf:pdf},
isbn = {0004-8402},
